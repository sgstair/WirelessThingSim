<Window x:Class="SimpleWirelessSimualator.RunUnitTests"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SimpleWirelessSimualator"
        mc:Ignorable="d"
        Title="Run Unit Tests" Height="500" Width="800">
    <Grid>
        <WrapPanel Margin="10,10,0,0">
            <ComboBox x:Name="comboSelectTest" HorizontalAlignment="Left" VerticalAlignment="Top" Width="353">
            </ComboBox>
            <Label Content="Iterations:"/>
            <TextBox x:Name="textIterations" Height="23" TextWrapping="Wrap" Text="TextBox" Width="120" TextChanged="textIterations_TextChanged"/>
            <Button x:Name="btnStart" Content="Start" Height="23" Width="75" Click="btnStart_Click"/>
        </WrapPanel>
        <ListBox x:Name="listBox" Margin="10,41,10,10"/>

    </Grid>
</Window>
